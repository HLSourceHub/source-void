"SDK_Aftershock_dx9"
{
	"<dx90"
	{
		"$fallbackmaterial" "models\props_combine\sphere_dx8"
	}

	"$basetexture" "_rt_worldtexture0"
	"$normalmap" "models/effects/rift/aftershock-normal"
//	"$normalmap" "dev/water_normal"

	"$refractamount" "0.003"
	"$colortint" "[1.0 1.3 1.6]"

	"$SilhouetteThickness" "0.2"
	"$SilhouetteColor" "[1.0 1.3 1.6]"
	"$GroundMin" "-1.0"
	"$GroundMax" "-1.0"
	"$BlurAmount" "0.0"

	"$time" "0.0"
	"Proxies"
	{
		"AnimatedTexture"
		{
			"animatedtexturevar" "$normalmap"
			"animatedtextureframenumvar" "$bumpframe"
			"animatedtextureframerate" 30.00
		}
		"TextureScroll"
		{
			"texturescrollvar" "$bumptransform"
			"texturescrollrate" .05
			"texturescrollangle" 45.00
		}
		"TextureScroll"
		{
			"texturescrollvar" "$basetexturetransform"
			"texturescrollrate" 1
			"texturescrollangle" 0.00
		}
		"CurrentTime"
		{
			"resultVar" "$time"
		}
		
